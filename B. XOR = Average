#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
    int tt;
    cin>>tt;
    while (tt--) {
        int n;
        cin>>n;
        if (n&1) {
            for(int i=1;i<=n;i++) {
                cout<<5<<' ';
            }
            cout<<endl;
        } else {
            for(int i=1;i<=n-2;i++) {
                cout<<2<<' ';
            }
            cout<<"1 3"<<endl;
        }
    }
}
