#include <bits/stdc++.h>
using namespace std;
#define ll long long

bool isPerfectSquare(ll num) {
    ll root = sqrt(num);
    return root * root == num;
}

int main() {
    int tt;
    cin>>tt;
    while (tt--) {
        int n,a,b;
        string s;
        cin>>n>>a>>b>>s;

        int x=0,y=0,total=100;
        while (total--) {
            for (int i=0;i<n;i++) {
                if (s[i] == 'N') {
                    y++;
                } else if (s[i] == 'E') {
                    x++;
                } else if (s[i] == 'S') {
                    y--;
                } else if (s[i] == 'W') {
                    x--;
                }
                if (x==a && y==b) {
                    cout<<"YES"<<endl;
                    goto skip;
                }
            }
        }
        cout<<"NO"<<endl;
        skip:
           continue;
    }
}
